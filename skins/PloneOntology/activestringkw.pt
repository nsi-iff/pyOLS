<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone">
  <head><title></title></head>
  <body>

    <!-- ActiveString Widgets -->
    <metal:view_macro define-macro="view"
		      tal:replace="structure accessor" />

    <metal:define define-macro="edit">
      <metal:use use-macro="field_macro | here/widgets/field/macros/edit">
<div metal:fill-slot="widget_body"
	     tal:define="searchterm python: request.get('searchterm','');
	                 SearchKW python: request.get('SearchKW',None);
			 accessor python:getattr(here, field.accessor);
			 old python: accessor() or [];
			 ">
	<input type="text"
	       name=""
	       value=""
	       id=""
	       size="30"
	       tabindex="#"
               onfocus="loadHandler(this);"
               onkeypress="activeStringStart(this)"
	       tal:attributes="name fieldName;
	                       value python: request.get('SearchKW','');
                               id fieldName;
                 	       size widget/size;
	                       tabindex tabindex/next;"
	       />

<input type="hidden" name="skw" id="skw" value="" />

<div tal:attributes="id string:activeresult-${fieldName}" style="display: none;">
          <div tal:attributes="id string:activeresult-shadow-${fieldName}">
          </div>
        </div>
</div>
      </metal:use>
    </metal:define>

    <div metal:define-macro="search">
      <div metal:use-macro="here/widgets/field/macros/edit">
      </div>
    </div>
  </body>

</html>


